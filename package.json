{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "dev": "npm run start",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.20",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.20",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/js-cookie": "^2.2.6",
    "@types/moment": "^2.13.0",
    "js-cookie": "^2.2.1",
    "moment": "^2.29.1"
  }
}
